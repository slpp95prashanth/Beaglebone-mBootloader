menu "shell commands"
    depends on SHELL

config SHELL_REGDUMP
    bool "Regdump command"
    default y

config SHELL_MD
    bool "md command"
    default y

config SHELL_MM
    bool "mm command"
    default y

config SHELL_GPIO
    depends on GPIO
    bool "gpio command"
    default y

config SHELL_RESET
    bool "reset command"
    default y

endmenu

